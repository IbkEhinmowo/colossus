{
  "manifest_version": 3,
  "name": "Auto Marketplace Scanner",
  "version": "1.0",
  "description": "Automatically scans marketplace listings every minute",
  "permissions": ["contextMenus", "scripting", "activeTab", "tabs", "alarms"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
