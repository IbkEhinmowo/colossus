{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A basic Chrome extension with context menu",
  "permissions": ["contextMenus", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
